<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".activity.TicketInfoActivity">

    <View
        android:id="@+id/vector"
        android:layout_width="450dp"
        android:layout_height="299dp"
        android:layout_marginBottom="63dp"
        android:layout_marginTop="62dp"
        android:background="@drawable/screen_view"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.51" />

    <View
        android:id="@+id/ticketInfoBox"
        android:layout_width="223dp"
        android:layout_height="283dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ticket_info_box_shape"
        app:layout_constraintBottom_toBottomOf="@id/vector"
        app:layout_constraintEnd_toEndOf="@+id/vector"
        app:layout_constraintLeft_toLeftOf="@id/vector"
        app:layout_constraintTop_toTopOf="@id/vector"
        app:layout_constraintVertical_bias="0.833" />

    <View
        android:id="@+id/ticketInfoBoxHeader"
        android:layout_width="223dp"
        android:layout_height="60dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/ticket_info_box_header"
        app:layout_constraintEnd_toEndOf="@+id/vector"
        app:layout_constraintTop_toTopOf="@+id/ticketInfoBox" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:text="Test"
        app:layout_constraintEnd_toEndOf="@+id/ticketInfoBox"
        app:layout_constraintStart_toStartOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/ticketInfoBoxHeader" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/ticketInfoBoxHeader" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="45dp"
        android:layout_height="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="16:25"
        android:textColor="@android:color/black"
        android:textColorHighlight="@android:color/black"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/circleView2"
        app:layout_constraintTop_toTopOf="@+id/circleView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="40dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/ticketInfoBoxHeader" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/ticketInfoBox"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <View
        android:id="@+id/circleView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@+id/vector"
        app:layout_constraintEnd_toStartOf="@+id/ticketInfoBox"
        app:layout_constraintStart_toStartOf="@+id/vector" />

    <View
        android:id="@+id/circleView2"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/time_view_shape"
        app:layout_constraintStart_toStartOf="@+id/vector"
        app:layout_constraintTop_toTopOf="@+id/vector" />

</android.support.constraint.ConstraintLayout>